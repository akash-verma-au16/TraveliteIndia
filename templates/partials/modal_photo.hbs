<div class='modal fade' id='modal-photo' tabindex='-1'>
  <div class='modal-dialog'>
    <div class='modal-content'>
      <div class='modal-header'>
        <h5 class='modal-title'>Upload the Image</h5>
        <button
          type='button'
          class='btn-close'
          data-bs-dismiss='modal'
        ></button>
      </div>
      <div class='modal-body'>
        {{#with agency}}
          <form
            action='/agency/{{_id}}/photo'
            method='POST'
            enctype='multipart/form-data'
          >
            <input
              type='file'
              id='agency_photo'
              class='form-control'
              name='file'
              autofocus=''
              required=''
            />
            <div class='modal-footer'>
              <button
                id='upload-btn'
                type='submit'
                class='btn btn-success float-end'
              >UPLOAD</button>
              <button
                type='button'
                class='btn btn-secondary'
                data-bs-dismiss='modal'
              >Close</button>
            </div>
          </form>
        {{/with}}
      </div>
    </div>
  </div>
</div>