<div id='logreg-forms' class='m-3 mx-auto container'>
  <form class='form-signup p-5' action='/agency' method='POST'>
    <h1 class='h3 mb-3 font-weight-normal'>Fill the Agency Details:</h1>
    <input
      type='text'
      id='name'
      name='name'
      class='form-control'
      placeholder='Agency name'
      required=''
      autofocus=''
    />
    <input
      type='text'
      id='description'
      name='description'
      class='form-control'
      placeholder='Description'
      required=''
      autofocus=''
    />
    <input
      type='text'
      id='address'
      name='address'
      class='form-control'
      placeholder='Agency Address'
      required=''
      autofocus=''
    />
    <input
      type='text'
      id='website'
      name='website'
      class='form-control'
      placeholder='Agency Website'
      required
      autofocus=''
    />
    <input
      type='email'
      id='email'
      name='email'
      class='form-control'
      placeholder='Email address'
      required
      autofocus=''
    />
    <input
      type='tel'
      id='phone'
      name='phone'
      class='form-control'
      placeholder='Phone Number'
      required
      autofocus=''
    />

    <button class='btn btn-primary w-100 mt-5' type='submit'>
      <i class='fas fa-user-plus'></i>
      Create New Agency
    </button>
  </form>
</div>